<body class="pricing-page">    
  <div class="wrapper">
    <!-- ******PRICE PLAN****** --> 
    <section id="price-plan" class="price-plan section">
      <div class="container text-center">
        <h2 class="title">30 day FREE trial with all plans!</h2>
        <p class="intro">CopperService's pricing is simple and you can change your plan at any time. </p>
        <div class="price-cols row">

          <%= form_tag new_user_registration_path ,:method=>"get" do %>  
            <div class="item col-md-4 col-sm-4 col-xs-12">
              <h3 class="heading first">SOLO</h3>
              <div class="content">
                <p class="price-figure"><span class="currency">$</span><span class="number">19.95</span><span class="unit">/month</span></p>
                <ul class="list-unstyled feature-list">
                  <li>1 user</li>
                  <li>Customer Unlimited</li>
                  <li>Email Notification yes</li>
                  <li>10 Gb</li>
                </ul>
                <%= hidden_field_tag "plan_type" ,"solo"%>
                <%= button_tag :class=>"btn btn-cta btn-cta-primary" do %>Sign Up<br/>
                <span class="extra">30 day free trial</span>
                <%end%>
              </div><!--//content-->
            </div><!--//item--> 
          <%end%> 

          <%= form_tag new_user_registration_path ,:method=>"get" do %>        
            <div class="item col-md-4 col-sm-4 col-xs-12">
              <h3 class="heading first">TEAM <span class="label label-custom">Popular</span></h3>
              <div class="content">
                <p class="price-figure"><span class="currency">$</span><span class="number">29.95</span><span class="unit">/month</span></p>
                <ul class="list-unstyled feature-list">
                  <li>5 users</li>
                  <li>Customer Unlimited</li>
                  <li>Email Notification yes</li>
                  <li>25 Gb</li>
                </ul>
                <%= hidden_field_tag "plan_type" ,"team"%>
                <%= button_tag :class=>"btn btn-cta btn-cta-primary" do %>Sign Up<br/>
                <span class="extra">30 day free trial</span>
                <%end%>
              </div><!--//content-->
            </div><!--//item-->  
          <%end%>

          <%= form_tag new_user_registration_path ,:method=>"get" do %>    
            <div class="item col-md-4 col-sm-4 col-xs-12">
              <h3 class="heading first">BUSINESS</h3>
              <div class="content">
                <p class="price-figure"><span class="currency">$</span><span class="number">49.95</span><span class="unit">/month</span></p>
                <ul class="list-unstyled feature-list">
                  <li>20 Each additional user $2</li>
                  <li>Customer Unlimited</li>
                  <li>Email Notification yes</li>
                  <li>100 Gb</li>
                </ul>
                <%= hidden_field_tag "plan_type" ,"business"%>
                <%= button_tag :class=>"btn btn-cta btn-cta-primary" do %>Sign Up<br/>
                <span class="extra">30 day free trial</span>
                <%end%>
              </div><!--//content-->
            </div><!--//item--> 
          <%end%>  


        </div><!--//row-->
      </div><!--//container-->
    </section><!--//price-plan-->
        
    <!-- ******FAQ****** --> 
